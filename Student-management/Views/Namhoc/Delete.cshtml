@model Student_management.Models.Namhoc

@{
    ViewData["Title"] = "Xóa năm học";
}

<h1>Bạn có chắc muốn xóa năm học này?</h1>
<hr />
<dl class="row">
    <dt class="col-sm-2">Mã Năm học</dt>
    <dd class="col-sm-10">@Model.MaNamHoc</dd>

    <dt class="col-sm-2">Tên Năm học</dt>
    <dd class="col-sm-10">@Model.TenNamHoc</dd>
</dl>

<form asp-action="Delete" method="post">
    <input type="hidden" asp-for="MaNamHoc" />
    <button type="submit" class="btn btn-danger">Xóa</button>
    <a asp-action="Index" class="btn btn-secondary">Quay lại</a>
</form>
